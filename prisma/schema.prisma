generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model antutu {
  id         BigInt   @id @default(autoincrement())
  created_at DateTime @default(now()) @db.Timestamptz(6)
  name       String?
  cpu        String?
  gpu        String?
  mem        String?
  ux         String?
  total      String?
}

model smartphones {
  id           BigInt    @default(autoincrement())
  created_at   DateTime  @default(now()) @db.Timestamptz(6)
  name         String
  brand        String?
  dxomarkScore BigInt?
  segmentPrice String?
  ram          String?
  launchDate   DateTime? @db.Date
  image        String?
  battery      String?
  cameraPixel  String?
  videoPixel   String?
  displaySize  String?
  displayRes   String?
  storage      String?
  price        String?
  photo        String?
  video        String?
  bokeh        String?
  preview      String?
  zoom         String?

  @@id([id, name])
}
